<h1>Articles#index</h1>

<%= link_to 'New Article', new_article_path %>
<%= link_to 'Generate FAKE 10 articles!', fake_path %>

<ol>
  <div class="cards">
    <% @articles.each do |article| %>
      <div class="card">
        <li><h2><%= link_to article.title, article_path(article) %></h2></li>
        <p><%= article.content %></p>
      </div>
    <% end %>
  </div>
</ol>
